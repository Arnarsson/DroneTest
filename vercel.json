{
  "buildCommand": "./debug-build.sh",
  "outputDirectory": "frontend/.next",
  "installCommand": "echo 'Using custom build script'",
  "framework": null,
  "functions": {
    "api/index.py": {
      "runtime": "python@3.11",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    }
  ]
}